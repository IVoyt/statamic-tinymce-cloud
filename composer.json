{
  "name": "mitydigital/statamic-tinymce-cloud",
  "description": "A Statamic fieldtype for TinyMCE using Tiny Cloud for Statamic 4 and 5",
  "type": "statamic-addon",
  "keywords": [
    "statamic",
    "tinymce",
    "tinymce cloud",
    "tiny cloud"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MityDigital\\StatamicTinymceCloud\\": "src"
    }
  },
  "authors": [
    {
      "name": "Marty Friedel"
    }
  ],
  "support": {
    "email": "dev@mity.com.au"
  },
  "require": {
    "php": "^8.0",
    "statamic/cms": "^4.0|^5.0"
  },
  "extra": {
    "statamic": {
      "name": "TinyMCE Cloud",
      "description": "A Statamic fieldtype for TinyMCE using Tiny Cloud for Statamic 4 and 5."
    },
    "laravel": {
      "providers": [
        "MityDigital\\StatamicTinymceCloud\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
